{
  "name": "veramo-backend-template",
  "version": "1.0.0",
  "description": "A template for a nodejs typescript app that uses a veramo agent",
  "private": "true",
  "type": "module",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "module": "lib/index.module.js",
  "types": "lib/index.d.ts",
  "repository": "git@github.com:mirceanis/veramo-backend-template.git",
  "author": "Mircea Nistor",
  "license": "Apache-2.0",
  "files": [
    "lib",
    "src",
    "LICENSE"
  ],
  "scripts": {
    "start": "node --no-warnings --loader ts-node/esm ./dist/src/bin/www.ts",
    "test": "cross-env NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "build": "tsc -p .",
    "start:docker": "node --no-warnings --loader ts-node/esm ./src/server.ts",
    "local": "yarn ts-node --esm ./src/server.ts",
    "remote": "node --no-warnings --loader ts-node/esm src/issue-remote-verify-local.ts",
    "server": "node --no-warnings --loader ts-node/esm src/server.ts",
    "format": "prettier -w '**/**.{js,ts,json,md,yml}'"
  },
  "devDependencies": {
    "@babel/core": "7.23.3",
    "@babel/preset-env": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@jest/globals": "29.7.0",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.8",
    "@types/swagger-ui-express": "^4.1.6",
    "cross-env": "7.0.3",
    "jest": "29.7.0",
    "jest-config": "29.7.0",
    "prettier": "3.1.0",
    "ts-jest": "29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.454.0",
    "@aws-sdk/lib-dynamodb": "^3.454.0",
    "@aws-sdk/types": "^3.451.0",
    "@types/node": "^20.9.2",
    "@veramo/core": "^5.1.2",
    "@veramo/credential-w3c": "^5.1.2",
    "@veramo/data-store": "^5.1.2",
    "@veramo/did-comm": "^5.5.3",
    "@veramo/did-jwt": "^5.5.3",
    "@veramo/did-manager": "^5.1.2",
    "@veramo/did-provider-ethr": "^5.1.2",
    "@veramo/did-provider-key": "^5.5.3",
    "@veramo/did-provider-peer": "^5.5.3",
    "@veramo/did-provider-web": "^5.1.2",
    "@veramo/did-resolver": "^5.1.2",
    "@veramo/key-manager": "^5.1.2",
    "@veramo/kms-local": "^5.1.2",
    "@veramo/remote-server": "^5.5.3",
    "@veramo/selective-disclosure": "^5.5.3",
    "cookie-parser": "^1.4.6",
    "did-jwt": "^7.4.5",
    "enjoi": "^9.0.1",
    "ethr-did-resolver": "^7.0.2",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "sqlite3": "^5.1.2",
    "swagger-ui-express": "^5.0.0",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.17",
    "web-did-resolver": "^2.0.21",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "resolutions": {
    "jsonld": "npm:@digitalcredentials/jsonld@^6.0.0"
  }
}
